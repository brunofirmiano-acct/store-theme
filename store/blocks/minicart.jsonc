{
  "add-to-cart-button": {
    "props": {
      "addToCartFeedback": "customEvent",
      "customPixelEventId": "add-to-cart-button"
    }
  },

  "minicart.v2": {
    "props": {
      "variation": "popupWithLink",
      "openOnHover": true,
      "MinicartIcon": "icon-cart#minicart-icon",
